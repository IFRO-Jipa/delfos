<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="272.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <TableView fx:id="tbPerguntas" prefHeight="269.0" prefWidth="415.0" BorderPane.alignment="CENTER">
         <columns>
            <TableColumn fx:id="columnNome" prefWidth="176.0" text="Nome" />
            <TableColumn fx:id="columnTipo" prefWidth="187.0" text="Tipo" />
            <TableColumn fx:id="columnAcao" minWidth="0.0" prefWidth="49.0" resizable="false" text="Ação" />
         </columns>
      </TableView>
   </center>
   <top>
      <GridPane prefHeight="90.0" prefWidth="396.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="248.0" minWidth="10.0" prefWidth="248.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="180.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="60.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adicionar pergunta" />
            <TextField prefHeight="25.0" prefWidth="195.0" promptText="Nome da pergunta" GridPane.rowIndex="1">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </TextField>
            <ComboBox prefHeight="25.0" prefWidth="210.0" promptText="Tipo de pergunta" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" text="Add" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
         <opaqueInsets>
            <Insets left="10.0" />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets left="10.0" />
         </BorderPane.margin>
      </GridPane>
   </top>
</BorderPane>
